{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "dist/dashboard.js",
  "module": "src/index.js",
  "scripts": {
    "test": "mocha-webpack \"src/**/*.test.js\" --webpack-config webpack.test.config.js",
    "test:watch": "mocha-webpack --watch \"src/**/*.test.js\" --webpack-config webpack.test.config.js",
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "html-coverage": "cross-env NODE_ENV=test nyc --reporter=html mocha-webpack \"src/**/*.test.js\" --webpack-config webpack.test.config.js",
    "generate-doc": "jsdoc -c jsdoc.conf.json",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "billboard.js": "^1.2.0",
    "d3": "^4.12.2",
    "jq-web": "^0.1.3",
    "lodash": "^4.17.4",
    "sassline": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.8",
    "docdash": "^0.4.0",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.9.0",
    "fake-style-loader": "^1.0.1",
    "file-loader": "^1.1.6",
    "google-fonts-webpack-plugin": "^0.4.4",
    "inject-loader": "^3.0.1",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.3.0",
    "jsdom": "^7.0.2",
    "mocha": "^4.1.0",
    "mocha-jsdom": "^1.1.0",
    "mocha-webpack": "^1.0.1",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "sass-loader": "^6.0.6",
    "should": "^13.2.0",
    "should-sinon": "0.0.6",
    "sinon": "^4.1.4",
    "style-loader": "^0.19.1",
    "stylish": "^1.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
